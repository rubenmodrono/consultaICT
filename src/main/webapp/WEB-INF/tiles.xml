<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
    <definition name="basic"
        template="/WEB-INF/pages/layouts/basicLayout.jsp">
        <put-attribute name="head" value="/WEB-INF/pages/common/head.jsp" />
        <put-attribute name="header" value="/WEB-INF/pages/common/header.jsp" />
        <put-attribute name="tabs" value="/WEB-INF/pages/common/tabs.jsp" />
        <put-attribute name="body" value="" />
        <put-attribute name="footer" value="/WEB-INF/pages/common/footer.jsp" />
    </definition>
    
    <definition name="complex"
        template="/WEB-INF/pages/layouts/complexLayout.jsp">
        <put-attribute name="head" value="/WEB-INF/pages/common/head.jsp" />
        <put-attribute name="header" value="/WEB-INF/pages/common/header.jsp" />
        <put-attribute name="tabs" value="/WEB-INF/pages/common/tabs.jsp" />
        <put-attribute name="menu" value="/WEB-INF/pages/common/menu.jsp" />
        <put-attribute name="body" value="" />
        <put-attribute name="footer" value="/WEB-INF/pages/common/footer.jsp" />
    </definition>
 
    <definition name="home" extends="complex">
        <put-attribute name="body" value="/WEB-INF/pages/home.jsp" />
    </definition>
    
     <definition name="volcado" extends="basic">
        <put-attribute name="body" value="/WEB-INF/pages/import.jsp" />
    </definition>
 
</tiles-definitions>